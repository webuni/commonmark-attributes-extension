{
    "name": "webuni/commonmark-attributes-extension",
    "type": "commonmark-extension",
    "description": "The attributes extension adds a syntax to define attributes on the various HTML elements in CommonMark PHP implementation.",
    "keywords": ["markdown","attributes","commonmark"],
    "homepage": "https://github.com/webuni/commonmark-attributes-extension",
    "license": "MIT",
    "authors": [
        {
            "name": "Martin Haso≈à",
            "email": "martin.hason@gmail.com"
        },
        {
            "name": "Webuni s.r.o.",
            "homepage": "https://www.webuni.cz"
        }
    ],
    "require": {
        "php": "^7.1",
        "league/commonmark": "^1.0"
    },
    "require-dev": {
        "friendsofphp/php-cs-fixer": "^2.14",
        "phpstan/phpstan": "~0.11",
        "phpunit/phpunit": "^7.0|^8.0",
        "vimeo/psalm": "^3.5",
        "symfony/var-dumper": "^4.3",
        "league/commonmark-ext-table": "^1.0|^2.0"
    },
    "autoload": {
        "psr-4": {
            "Webuni\\CommonMark\\AttributesExtension\\": "src"
        }
    },
    "extra": {
        "branch-alias": {
            "dev-master": "1.0-dev"
        }
    },
    "abandoned": "league/commonmark"
}
